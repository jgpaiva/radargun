<bench-config>
   <master
         bindAddress="${127.0.0.1:master.address}"
         port="${21031:master.port}"/>
   <benchmark
         initSize="${1:Islaves}"
         maxSize="${1:slaves}"
         increment="1">
      <DestroyWrapper
            runOnAllSlaves="true"/>
      <StartCluster
            staggerSlaveStartup="true"
            delayAfterFirstSlaveStarts="5000"
            delayBetweenStartingSlaves="1000"/>
      <ClusterValidation
            passiveReplication="false"
            partialReplication="false"/>
      <ClearCluster />
      <WebSessionWarmup
            passiveReplication="false"
            noContentionEnabled="false"
            transactionSize="100"
            numberOfKeys="1000"
            sizeOfValue="1000"
            numOfThreads="2"/>
      <ResetStats />
      <WebSessionBenchmark
            perThreadSimulTime="30000000000"
            opsCountStatusLog="5000"
            numberOfKeys="1000"
            sizeOfValue="1000"
            statsSamplingInterval="5000"
            numOfThreads="2"
            writeOperationPercentage="50"
            writeTransactionPercentage="100"
            lowerBoundOp="10"
            upperBoundOp="10"
            coordinatorParticipation="true"
            noContentionEnabled="false"/>
      <CacheSize
            statName="CACHE_SIZE" />
      <CsvReportGeneration/>
   </benchmark>
   <products>
      <infinispan4>
         <config name="ispn.xml"/>
      </infinispan4>
   </products>
   <reports>
      <report name="Reports" />
   </reports>
</bench-config>