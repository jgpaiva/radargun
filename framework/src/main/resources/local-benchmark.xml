<local-bench-config>

    <benchmark>
        <PutGetWarmup
                passiveReplication="false"
                noContention="false"
                numberOfKeys="1000"
                sizeOfValue="1000"
                numOfThreads="2"
                transactionSize="50"
                />
        <PutGet
                perThreadSimulTime="300"
                writeTxWorkload="10,10;10,10"
                readTxWorkload="20"
                writeTransactionPercentage="50"
                coordinatorParticipation="true"
                noContention="false"
                numberOfKeys="1000"
                sizeOfValue="1000"
                numOfThreads="2"
                />
    </benchmark>

    <products>
        <infinispan4>
            <config
                    name="local/read-committed.xml"/>
        </infinispan4>
    </products>


    <reports>
        <report name="read-committed">
            <item
                    product="infinispan4"
                    config="local/read-committed.xml"/>
        </report>
    </reports>
</local-bench-config>
      